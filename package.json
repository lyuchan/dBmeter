{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^28.1.4",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "serialport": "^12.0.0"
  },
  "build": {
    "appId": "com.TallyLight.app",
    "productName": "TallyLight",
    "directories": {
      "output": "pack"
    },
    "files": [
      "**/*",
      "!.vscode",
      "build/",
      "data/"
    ],
    "asar": false,
    "win": {
      "icon": "./icon.png"
    }
  }
}
